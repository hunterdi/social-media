  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^services/services(|/.*)$": "<rootDir>/libs/services/src/$1",
      "^database-mongo/database-mongo(|/.*)$": "<rootDir>/libs/database-mongo/src/$1",
      "^database-postgres/database-postgres(|/.*)$": "<rootDir>/libs/database-postgres/src/$1",
      "^auth/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^Tools/tools(|/.*)$": "<rootDir>/libs/tools/src/$1",
      "^database-elasticsearch/database-elasticsearch(|/.*)$": "<rootDir>/libs/database-elasticsearch/src/$1",
      "^Logger/logger(|/.*)$": "<rootDir>/libs/logger/src/$1",
      "^service-logger/service-logger(|/.*)$": "<rootDir>/libs/service-logger/src/$1",
      "^dtos/dtos(|/.*)$": "<rootDir>/libs/dtos/src/$1",
      "^validators/validators(|/.*)$": "<rootDir>/libs/validators/src/$1",
      "^mappers/mappers(|/.*)$": "<rootDir>/libs/mappers/src/$1",
      "^@app/kafka(|/.*)$": "<rootDir>/libs/kafka/src/$1",
      "^@app/configuration(|/.*)$": "<rootDir>/libs/configuration/src/$1"
    }
