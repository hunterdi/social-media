  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "api:start": "nest start api",
    "api:start:dev": "nest start api --watch",
    "api:start:debug": "nest start api --debug --watch",
    "api:start:prod": "node dist/main",
    "sga:start": "nest start sga",
    "sga:start:dev": "nest start sga --watch",
    "sga:start:debug": "nest start sga --debug --watch",
    "sga:start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/social-media/test/jest-e2e.json",
    "services:up": "Make up",
    "services:down": "Make down",
    "typeorm:postgres": "typeorm-ts-node-esm -d ./libs/database-postgres/src/config/ormconfig.ts",
    "typeorm:cli": "typeorm-ts-node-esm",
    "postgres:migration:generate": "npm run typeorm:postgres migration:generate ./libs/database-postgres/src/migrations/Migration_Generated",
    "postgres:migration:create": "npm run typeorm:cli migration:create ./libs/database-postgres/src/migrations/Migration_Created",
    "postgres:migration:run": "npm run typeorm:postgres migration:run",
    "postgres:migration:revert": "npm run typeorm:postgres migration:revert",
    "postgres:migration:show": "npm run typeorm:postgres migration:show",
    "lib:create": "nest g library",
    "app:create": "nest g app",
    "postgres:seed": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js -c connPostgres -r ./libs/database-postgres/src/config -n ormconfig.ts",
    "postgres:seed:config": "yarn postgres:seed config",
    "postgres:seed:run": "yarn postgres:seed seed"
  },
